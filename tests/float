//      Example program : FLOAT OPERATIONS

LET pi = 3.14
LET e  = 2.718

LET A = pi * e
LET B = pi * 2.0

LET nl = "\n"
PRINT nl
PRINT "Pi is : "
PRINT pi
PRINT nl

PRINT nl
PRINT "e is : "
PRINT e
PRINT nl

PRINT nl
PRINT "Pi * e is : "
PRINT A
PRINT nl

PRINT nl
PRINT "Pi * 2.0 is : "
PRINT B
PRINT nl

PRINT nl
LET N = 5.5 + 1.0 * 5 / (2.2 * 2.0)
PRINT "[ N eq 5.5 + 1.0 * 5 / (2.2 * 2.0)] : "
PRINT N
PRINT nl

PRINT nl
LET X = 5.5 + 5.5 * 5 / (3.0 * 4.0)
PRINT "[ X eq 5.5 + 5.5 * 5 / (3.0 * 4.0)] : "
PRINT X
PRINT nl

END
